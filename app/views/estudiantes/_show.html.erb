<%- model_class = Estudiante -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<div class='team'>
  <p><b>Id:</b> <%= @estudiante.id %></p>
  <p><b>Nombre:</b> <%= @estudiante.nombre %></p>
  <p><b>Cedula:</b> <%= @estudiante.cedula %></p>
  <p><b>Email:</b> <%= @estudiante.email %></p>
  <p><b>Direccion:</b> <%= @estudiante.direccion %></p>
  <p><b>Fecha Nacimiento:</b> <%= @estudiante.fcha_nacimineto %></p>

</div>